{
  "name": "cote",
  "version": "0.13.2",
  "keywords": [
    "microservices",
    "zero-configuration",
    "zero-conf",
    "mesh",
    "network",
    "auto discovery",
    "scalable",
    "fault-tolerant"
  ],
  "description": "A Node.js library for building zero-configuration microservices.",
  "scripts": {
    "test": "ava",
    "build": "babel src --presets babel-preset-es2015 -D -d dist -s",
    "clean": "rm -rf dist",
    "prepublish": "npm run build"
  },
  "author": {
    "name": "Armagan Amcalar",
    "email": "armagan@amcalar.com"
  },
  "main": "dist/index.js",
  "licenses": "MIT",
  "repository": "dashersw/cote",
  "engines": {
    "node": ">=v0.10.0"
  },
  "dependencies": {
    "@dashersw/axon": "2.0.3",
    "charm": "0.1.2",
    "colors": "0.x.x",
    "eventemitter2": "0.4.x",
    "lodash": "4.13.1",
    "node-discover": "0.4.0",
    "node-uuid": "1.4.7",
    "portfinder": "1.0.13",
    "socket.io": "1.7.3"
  },
  "devDependencies": {
    "async": "^2.3.0",
    "async-q": "^0.3.1",
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "commander": "2.2.0",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.1",
    "humanize-number": "0.0.2",
    "log-suppress": "^1.0.3"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6
    },
    "extends": ["google"],
    "rules": {
      "require-jsdoc": "off",
      "new-cap": "off",
      "guard-for-in": "off",
      "indent": ["error", 4]
    }
  }
}
